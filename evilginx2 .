[32m+[m[32mfunc addRandomDelay() {[m
[32m+[m[32m    seed := make([]byte, 1)[m
[32m+[m[32m    rand.Read(seed)[m
[32m+[m[32m    delay := time.Duration(50+int(seed[0])%200) * time.Millisecond // 50-250ms[m
[32m+[m[32m    time.Sleep(delay)[m
[32m+[m[32m}[m
